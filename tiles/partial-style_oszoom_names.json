{
  "motorway junction numbers": {
    "id": "motorway junction numbers",
    "type": "symbol",
    "source": "placenames",
    "source-layer": "names",
    "minzoom": 13,
    "filter": ["match", ["get", "type"], ["Motorway Junctions"], true, false],
    "layout": {
      "text-field": ["to-string", ["get", "name"]],
      "text-size": [
        "interpolate",
        ["linear"],
        ["zoom"],
        13,
        11,
        16,
        16,
        22,
        30
      ],
      "text-font": ["Source Sans Pro Regular"]
    },
    "paint": {
      "text-color": "#000000",
      "text-halo-color": "#ffffff",
      "text-halo-width": 10
    }
  },
  
